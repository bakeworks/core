{
  "env": {
    "development": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
          "node": "14.15.1"
          }
        }]
      ],
      "plugins": [
        "@babel/transform-runtime"
      ]
    },
    "browser": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "browsers": "last 2 versions, ie 10-11"
          },
          "modules": false
        }]
      ],
      "plugins": [
        "@babel/transform-runtime"
      ]
    },
    "module": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "14.15.1"
          },
          "modules": false
        }]
      ],
      "plugins": [
        "@babel/transform-runtime"
      ]
    }
  },
  "sourceMaps": true
}